<div class='container'>
    <div>
      <h3 id='new-header'>Edit list:</h3>
    </div>

    <% if @errors %>
      <p>Please fix the following errors:</p>
          <% @errors.each do |error| %>
          <li><%= error %></li>
          <% end %>
    <% end %>

    <div>
      <%= erb :'lists/_edit_form', locals: {list: @list} %>
    </div>
</div>
